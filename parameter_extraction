  import star.common.*;
  import star.base.neo.*;
  
  public class ExtractParameterValues extends StarMacro {
      public void execute() {
          Simulation simulation_0 = getActiveSimulation();
          GlobalParameterManager paramManager = simulation_0.get(GlobalParameterManager.class);
          
          // Iterate through all global parameters
          for (GlobalParameter param : paramManager.getGlobalParameters()) {
              if (param instanceof ScalarGlobalParameter) {
                  ScalarGlobalParameter scalarParam = (ScalarGlobalParameter) param;
                  
                  // Get parameter name
                  String paramName = scalarParam.getPresentationName();
                  
                  // Get parameter value
                  double paramValue = scalarParam.getQuantity().getRawValue();
                  
                  // Get parameter units
                  Units paramUnits = scalarParam.getQuantity().getUnits();
                  String unitsString = paramUnits != null ? paramUnits.getPresentationName() : "No Units";
                  
                  // Print parameter details
                  System.out.println("Parameter: " + paramName + 
                                     ", Value: " + paramValue + 
                                     ", Units: " + unitsString);
              }
          }
      }
  }
